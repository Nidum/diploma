<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nidum.nulp.yoso_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="nidum.nulp.yoso.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nidum.nulp.yoso.activity.HieroglyphAnimationActivity"
            android:label="@string/title_activity_kana_animation"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="nidum.nulp.yoso.TestNavigationActivityOMG"
            android:label="@string/title_activity_test_navigation_omg"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="nidum.nulp.yoso.activity.KanjiActivity" />
        <activity
            android:name="nidum.nulp.yoso.activity.RadicalsListActivity"
            android:label="@string/title_activity_radicals"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="nidum.nulp.yoso.activity.KanjiListActivity"></activity>
    </application>

</manifest>