<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nidum.nulp.yoso_project">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="nidum.nulp.yoso.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="nidum.nulp.yoso.activity.HieroglyphAnimationActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="nidum.nulp.yoso.activity.KanjiLevelListActivity" />
        <activity android:name="nidum.nulp.yoso.activity.RadicalsListActivity" />
        <activity android:name="nidum.nulp.yoso.activity.KanjiListActivity" />
        <activity android:name="nidum.nulp.yoso.activity.InstructionActivity" />
        <activity
            android:name="nidum.nulp.yoso.activity.SearchActivity"
            android:theme="@style/AppTheme.NoActionBar" />
    </application>

</manifest>